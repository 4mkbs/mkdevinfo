<?xml version="1.0" encoding="utf-8"?>
<resources>
    <data-extraction-rules>
        <cloud-backup>
            <!-- Exclude sensitive shared preferences -->
            <exclude domain="sharedpref" path="devinfo_preferences.xml" />
        </cloud-backup>
        <device-transfer>
            <!-- Allow transfer of app data -->
            <include domain="sharedpref" path="." />
        </device-transfer>
    </data-extraction-rules>
</resources>
